@model Warrior

<div class="card-body">
    <h4 class="card-title">@Model.WarriorName</h4>
    <table class="table">
        <tr>
            <td>HP</td>
            <td>@Model.Hp</td>
        </tr>
        <tr>
            <td>Attack Modifier</td>
            <td>@Model.AttackMod</td>
        </tr>
        <tr>
            <td>Defense</td>
            <td>@Model.Defense</td>
        </tr>
        <form asp-controller="Warrior" asp-action="DeleteWarrior" asp-route-id="@Model.WarriorId" method="post">
            <button class="btn-danger" type="submit">Delete</button>
        </form>
    </table>
</div>
